  <types xsi:type="occi:RecordType" name="tosca.datatypes.network.NetworkInfo">
    <recordFields name="network_id" type="//@types.1"/>
    <recordFields name="addresses" type="//@types.12"/>
    <recordFields name="network_name" type="//@types.1"/>
  </types>
  <types xsi:type="occi:RecordType" name="tosca.datatypes.network.PortInfo">
    <recordFields name="port_name" type="//@types.1"/>
    <recordFields name="network_id" type="//@types.1"/>
    <recordFields name="addresses" type="//@types.12"/>
    <recordFields name="mac_address" type="//@types.1"/>
    <recordFields name="port_id" type="//@types.1"/>
  </types>
  <types xsi:type="occi:RecordType" name="tosca.datatypes.Credential" documentation="The Credential type is a complex TOSCA data Type used when describing authorization credentials used to access network accessible resources.">
    <recordFields name="protocol" description="The optional protocol name." type="//@types.1"/>
    <recordFields name="keys" description="The optional list of protocol-specific keys or assertions." type="//@types.22"/>
    <recordFields name="token_type" default="The required token type." description="The required token type." type="//@types.1"/>
    <recordFields name="user" description="The optional user (name or ID) used for non-token based credentials." type="//@types.1"/>
    <recordFields name="token" description="The required token used as a credential for authorization or access to a networked resource." type="//@types.1"/>
  </types>
  <types xsi:type="occi:EnumerationType" name="protocolEnum">
    <literals name="udp"/>
    <literals name="tcp"/>
    <literals name="igmp"/>
  </types>
  <types xsi:type="occi:RecordType" name="tosca.datatypes.network.PortSpec">
    <recordFields name="protocol" required="true" type="//@types.32"/>
    <recordFields name="target_range"/>
    <recordFields name="source_range"/>
    <recordFields name="source" type="//@types.0"/>
    <recordFields name="target" type="//@types.0"/>
