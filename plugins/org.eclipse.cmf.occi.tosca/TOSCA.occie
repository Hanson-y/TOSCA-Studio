<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="TOSCA" scheme="http://occi/tosca#" description="Mon extension TOSCA">
  <mixins name="tosca.capabilities.Root" scheme="http://occi/tosca/toscacapabilitiesroot#" title="This is the default (root) TOSCA Capability Type definition that all other TOSCA Capability Types derive from."/>
  <mixins name="tosca.capabilities.Node" scheme="http://occi/tosca/toscacapabilitiesnode#" title="The Node capability indicates the base capabilities of a TOSCA Node Type." depends="//@mixins[term='tosca.capabilities.root']"/>
  <mixins name="tosca.capabilities.Endpoint" scheme="http://occi/tosca/toscacapabilitiesendpoint#" title="This is the default TOSCA type that should be used or extended to define a network endpoint capability. This includes the information to express a basic endpoint with a single port or a complex endpoint with multiple ports. By default the Endpoint is assumed to represent an address on a private network unless otherwise specified." depends="//@mixins[term='tosca.capabilities.root']">
    <attributes name="port_name" description="The optional name (or ID) of the network port this endpoint should be bound to." type="//@types.1"/>
    <attributes name="protocol" default="tcp" description="The name of the protocol (i.e., the protocol prefix) that the endpoint accepts (any OSI Layer 4-7 protocols) Examples: http, https, ftp, tcp, udp, etc." type="//@types.1"/>
    <attributes name="port" description="The optional port of the endpoint." type="//@types.0"/>
    <attributes name="initiator" default="source" description="The optional indicator of the direction of the connection." type="//@types.35"/>
    <attributes name="network_name" default="PRIVATE" description="The optional name (or ID) of the network this endpoint should be bound to. network_name: PRIVATE | PUBLIC |&lt;network_name> | &lt;network_id>" type="//@types.1"/>
    <attributes name="secure" default="false" description="Requests for the endpoint to be secure and use credentials supplied on the ConnectsTo relationship." type="//@types.3"/>
    <attributes name="ports" description="The optional map of ports the Endpoint supports (if more than one)." type="//@types.22"/>
    <attributes name="url_path" description="The optional URL path of the endpoint&#xe2;&#x20ac;&#x2122;s address if applicable for the protocol." type="//@types.1"/>
  </mixins>
  <mixins name="tosca.capabilities.Scalable" scheme="http://occi/tosca/toscacapabilitiesscalable#" title="This is the default TOSCA type that should be used to express a scalability capability for a node." depends="//@mixins[term='tosca.capabilities.root']">
    <attributes name="min_instances" default="1" description="This property is used to indicate the minimum number of instances that should be created for the associated TOSCA Node Template by a TOSCA orchestrator." type="//@types.7"/>
    <attributes name="max_instances" default="1" description="This property is used to indicate the maximum number of instances that should be created for the associated TOSCA Node Template by a TOSCA orchestrator." type="//@types.7"/>
    <attributes name="default_instances" default="1" description="An optional property that indicates the requested default number of instances that should be the starting number of instances a TOSCA orchestrator should attempt to allocate. Note: The value for this property MUST be in the range between the values set for &#xe2;&#x20ac;&#x2dc;min_instances&#xe2;&#x20ac;&#x2122; and &#xe2;&#x20ac;&#x2dc;max_instances&#xe2;&#x20ac;&#x2122; properties." type="//@types.7"/>
  </mixins>
  <mixins name="tosca.capabilities.network.Bindable" scheme="http://occi/tosca/toscacapabilitiesnetworkbindable#" depends="//@mixins[term='tosca.capabilities.node']"/>
  <mixins name="tosca.capabilities.Container" scheme="http://occi/tosca/toscacapabilitiescontainer#" title="The Container capability, when included on a Node Type or Template definition, indicates that the node can act as a container for (or a host for) one or more other declared Node Types." depends="//@mixins[term='tosca.capabilities.root']">
    <attributes name="num_cpus" description="Number of (actual or virtual) CPUs associated with the Compute node." type="//@types.8"/>
    <attributes name="disk_size" description="Size of the local disk available to applications running on the Compute node (default unit is MB)." type="//@types.28"/>
    <attributes name="cpu_frequency" description="Specifies the operating frequency of CPU's core.  This property expresses the expected frequency of one (1) CPU as provided by the property &#xe2;&#x20ac;&#x153;num_cpus&#xe2;&#x20ac;&#xfffd;." type="//@types.25"/>
    <attributes name="mem_size" description="Size of memory available to applications running on the Compute node (default unit is MB)." type="//@types.28"/>
  </mixins>
  <mixins name="tosca.capabilities.Endpoint.Database" scheme="http://occi/tosca/toscacapabilitiesendpointdatabase#" title="This is the default TOSCA type that should be used or extended to define a specialized database endpoint capability." depends="//@mixins[term='tosca.capabilities.endpoint']"/>
  <mixins name="tosca.capabilities.Endpoint.Public" scheme="http://occi/tosca/toscacapabilitiesendpointpublic#" title="This capability represents a public endpoint which is accessible to the general internet (and its public IP address ranges). This public endpoint capability also can be used to create a floating (IP) address that the underlying network assigns from a pool allocated from the application&#xe2;&#x20ac;&#x2122;s underlying public network.  This floating address is managed by the underlying network such that can be routed an application&#xe2;&#x20ac;&#x2122;s private address and remains reliable to internet clients." depends="//@mixins[term='tosca.capabilities.endpoint']">
    <attributes name="floating" default="false" description="Indicates that the public address should be allocated from a pool of floating IPs that are associated with the network." type="//@types.3"/>
    <attributes name="network_name" default="PUBLIC" type="//@types.1"/>
    <attributes name="dns_name" description="The optional name to register with DNS" type="//@types.1"/>
  </mixins>
  <mixins name="tosca.capabilities.Endpoint.Admin" scheme="http://occi/tosca/toscacapabilitiesendpointadmin#" title="This is the default TOSCA type that should be used or extended to define a specialized administrator endpoint capability." depends="//@mixins[term='tosca.capabilities.endpoint']">
    <attributes name="secure" default="true" type="//@types.3"/>
  </mixins>
  <mixins name="tosca.capabilities.OperatingSystem" scheme="http://occi/tosca/toscacapabilitiesoperatingsystem#" title="This is the default TOSCA type that should be used to express an Operating System capability for a node." depends="//@mixins[term='tosca.capabilities.root']">
    <attributes name="type" description="The Operating System (OS) type. Examples of valid values include: linux, aix, mac, windows, etc." type="//@types.1"/>
    <attributes name="distribution" description="The Operating System (OS) distribution. Examples of valid values for an &#xe2;&#x20ac;&#x153;type&#xe2;&#x20ac;&#xfffd; of &#xe2;&#x20ac;&#x153;Linux&#xe2;&#x20ac;&#xfffd; would include:  debian, fedora, rhel and ubuntu." type="//@types.1"/>
    <attributes name="version" description="The Operating System version." type="//@types.23"/>
    <attributes name="architecture" description="The Operating System (OS) architecture. Examples of valid values include: x86_32, x86_64, etc." type="//@types.1"/>
  </mixins>
  <mixins name="tosca.capabilities.Attachment" scheme="http://occi/tosca/toscacapabilitiesattachment#" title="This is the default TOSCA type that should be used or extended to define an attachment capability of a (logical) infrastructure device node (e.g., BlockStorage node)." depends="//@mixins[term='tosca.capabilities.root']"/>
  <mixins name="tosca.nodes.Root" scheme="http://occi/tosca/toscanodesroot#" title="This is the default (root) TOSCA Node Type that all other TOSCA nodes should extends. This allows all TOSCA nodes to have a consistent set of features for modeling and management (e.g, consistent definitions for requirements, capabilities, and lifecycle interfaces)." depends="//@mixins[term='tosca.capabilities.node'] //@mixins[term='tosca.interfaces.node.lifecycle.standard']">
    <applies href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
  </mixins>
  <mixins name="tosca.nodes.WebApplication" scheme="http://occi/tosca/toscanodeswebapplication#" title="The TOSCA WebApplication node represents a software application that can be managed and run by a TOSCA WebServer node. Specific types of web applications such as Java, etc. could be derived from this type." depends="//@mixins[term='tosca.nodes.root'] //@mixins[term='tosca.capabilities.endpoint']">
    <attributes name="context_root" description="The web application&#xe2;&#x20ac;&#x2122;s context root which designates the application&#xe2;&#x20ac;&#x2122;s URL path within the web server it is hosted on." type="//@types.1"/>
    <applies href="http://schemas.ogf.org/occi/platform#//@kinds[term='component']"/>
  </mixins>
  <mixins name="tosca.nodes.SoftwareComponent" scheme="http://occi/tosca/toscanodessoftwarecomponent#" title="The TOSCA SoftwareComponent node represents a generic software component that can be managed and run by a TOSCA Compute Node Type." depends="//@mixins[term='tosca.nodes.root']">
    <attributes name="admin_credential" description="The optional credential that can be used to authenticate to the software component." type="//@types.31"/>
    <attributes name="component_version" description="The optional software component&#xe2;&#x20ac;&#x2122;s version." type="//@types.23"/>
    <applies href="http://schemas.ogf.org/occi/platform#//@kinds[term='component']"/>
  </mixins>
  <mixins name="tosca.nodes.DBMS" scheme="http://occi/tosca/toscanodesdbms#" title="The TOSCA DBMS node represents a typical relational, SQL Database Management System software component or service.">
    <attributes name="port" description="the port the DBMS service will listen to for data and requests" type="//@types.7"/>
    <attributes name="root_password" description="the optional root password for the DBMS service" type="//@types.1"/>
    <constraints name="SourceMustBeDatabase"/>
    <depends href="#//@mixins[term='tosca.nodes.softwarecomponent']"/>
    <depends href="#//@mixins[term='tosca.capabilities.container']"/>
    <depends href="http://schemas.ogf.org/occi/platform#//@mixins[term='database']"/>
  </mixins>
  <mixins name="tosca.nodes.ObjectStorage" scheme="http://occi/tosca/toscanodesobjectstorage#" depends="//@mixins[term='tosca.nodes.root'] //@mixins[term='tosca.capabilities.endpoint']">
    <attributes name="size" type="//@types.27"/>
    <attributes name="name" required="true" type="//@types.1"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='storage']"/>
  </mixins>
  <mixins name="tosca.nodes.Compute" scheme="http://occi/tosca/toscanodescompute#" title="The TOSCA Compute node represents one or more real or virtual processors of software applications or services along with other essential local resources. Collectively, the resources the compute node represents can logically be viewed as a (real or virtual) &#xe2;&#x20ac;&#x153;server&#xe2;&#x20ac;&#xfffd;." depends="//@mixins[term='tosca.nodes.root'] //@mixins[term='tosca.capabilities.scalable'] //@mixins[term='tosca.capabilities.endpoint.admin'] //@mixins[term='tosca.capabilities.operatingsystem'] //@mixins[term='tosca.capabilities.container'] //@mixins[term='tosca.capabilities.network.bindable']">
    <attributes name="private_address" description="The primary private IP address assigned by the cloud provider that applications may use to access the Compute node." type="//@types.1"/>
    <attributes name="public_address" description="The primary public IP address assigned by the cloud provider that applications may use to access the Compute node." type="//@types.1"/>
    <attributes name="networks" type="//@types.22"/>
    <attributes name="ports" type="//@types.22"/>
    <constraints name="SourceMustBeSoftwareComponent"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='compute']"/>
  </mixins>
  <mixins name="tosca.nodes.LoadBalancer" scheme="http://occi/tosca/toscanodesloadbalancer#" title="The TOSCA Load Balancer node represents logical function that be used in conjunction with a Floating Address to distribute an application&#xe2;&#x20ac;&#x2122;s traffic (load) across a number of instances of the application (e.g., for a clustered or scaled application)." depends="//@mixins[term='tosca.nodes.root'] //@mixins[term='tosca.capabilities.endpoint.public']">
    <attributes name="algorithm" type="//@types.1"/>
  </mixins>
  <mixins name="tosca.nodes.WebServer" scheme="http://occi/tosca/toscanodeswebserver#" title="This TOSCA WebServer Node Type represents an abstract software component or service that is capable of hosting and providing management operations for one or more WebApplication nodes." depends="//@mixins[term='tosca.nodes.softwarecomponent'] //@mixins[term='tosca.capabilities.endpoint'] //@mixins[term='tosca.capabilities.endpoint.admin'] //@mixins[term='tosca.capabilities.container']">
    <constraints name="SourceMustBeWebApplication"/>
  </mixins>
  <mixins name="tosca.nodes.Database" scheme="http://occi/tosca/toscanodesdatabase#" title="The TOSCA Database node represents a logical database that can be managed and hosted by a TOSCA DBMS node." depends="//@mixins[term='tosca.nodes.root'] //@mixins[term='tosca.capabilities.endpoint.database']">
    <attributes name="password" description="the optional password for the DB user account" type="//@types.1"/>
    <attributes name="port" description="the port the underlying database service will listen to for data" type="//@types.7"/>
    <attributes name="name" required="true" description="the logical name of the database" type="//@types.1"/>
    <attributes name="user" description="the optional user account name for DB administration" type="//@types.1"/>
    <applies href="http://schemas.ogf.org/occi/platform#//@kinds[term='component']"/>
  </mixins>
  <mixins name="tosca.nodes.Container.Application" scheme="http://occi/tosca/toscanodescontainerapplication#" title="The TOSCA Container Application node represents an application that requires Container-level virtualization technology." depends="//@mixins[term='tosca.nodes.root']">
    <applies href="http://schemas.ogf.org/occi/platform#//@kinds[term='component']"/>
  </mixins>
  <mixins name="tosca.nodes.Container.Runtime" scheme="http://occi/tosca/toscanodescontainerruntime#" title="The TOSCA Container Runtime node represents operating system-level virtualization technology used to run multiple application services on a single Compute host." depends="//@mixins[term='tosca.nodes.softwarecomponent'] //@mixins[term='tosca.capabilities.scalable'] //@mixins[term='tosca.capabilities.container']"/>
  <mixins name="tosca.nodes.BlockStorage" scheme="http://occi/tosca/toscanodesblockstorage#" title="The TOSCA BlockStorage node currently represents a server-local block storage device (i.e., not shared) offering evenly sized blocks of data from which raw storage volumes can be created." depends="//@mixins[term='tosca.nodes.root'] //@mixins[term='tosca.capabilities.attachment']">
    <attributes name="size" description="The requested storage size (default unit is MB). Note: - Required when an existing volume (i.e., volume_id) is not available. - If volume_id is provided, size is ignored.  Resize of existing volumes is not considered at this time." type="//@types.26"/>
    <attributes name="volume_id" description="ID of an existing volume (that is in the accessible scope of the requesting application)." type="//@types.1"/>
    <attributes name="snapshot_id" description="Some identifier that represents an existing snapshot that should be used when creating the block storage (volume)." type="//@types.1"/>
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='storage']"/>
  </mixins>
  <mixins name="tosca.policies.Root" scheme="http://occi/tosca/toscapoliciesroot#" title="The TOSCA Policy Type all other TOSCA Policy Types derive from">
    <depends href="http://schemas.ogf.org/occi/sla#//@mixins[term='agreement_term']"/>
  </mixins>
  <mixins name="tosca.policies.Update" scheme="http://occi/tosca/toscapoliciesupdate#" title="The TOSCA Policy Type definition that is used to govern update of TOSCA nodes or groups of nodes." depends="//@mixins[term='tosca.policies.root']"/>
  <mixins name="tosca.policies.Placement" scheme="http://occi/tosca/toscapoliciesplacement#" title="The TOSCA Policy Type definition that is used to govern placement of TOSCA nodes or groups of nodes." depends="//@mixins[term='tosca.policies.root']"/>
  <mixins name="tosca.policies.Scaling" scheme="http://occi/tosca/toscapoliciesscaling#" title="The TOSCA Policy Type definition that is used to govern scaling of TOSCA nodes or groups of nodes." depends="//@mixins[term='tosca.policies.root']"/>
  <mixins name="tosca.policies.Performance" scheme="http://occi/tosca/toscapoliciesperformance#" title="The TOSCA Policy Type definition that is used to declare performance requirements for TOSCA nodes or groups of nodes." depends="//@mixins[term='tosca.policies.root']"/>
  <mixins name="tosca.groups.Root" scheme="http://occi/tosca/toscagroupsroot#" title="The TOSCA Group Type all other TOSCA Group Types derive from"/>
  <mixins name="tosca.interfaces.Root" scheme="http://occi/tosca/toscainterfacesroot#" title="The TOSCA root Interface Type all other TOSCA base Interface Types derive from."/>
  <mixins name="tosca.interfaces.relationship.Configure" scheme="http://occi/tosca/toscainterfacesrelationshipconfigure#" depends="//@mixins[term='tosca.interfaces.root']">
    <actions name="pre_configure_source" scheme="http://occi/tosca/pre_configure_source#" title="Operation to pre-configure the source endpoint."/>
    <actions name="pre_configure_target" scheme="http://occi/tosca/pre_configure_target#" title="Operation to pre-configure the target endpoint."/>
    <actions name="post_configure_source" scheme="http://occi/tosca/post_configure_source#" title="Operation to post-configure the source endpoint."/>
    <actions name="post_configure_target" scheme="http://occi/tosca/post_configure_target#" title="Operation to post-configure the target endpoint."/>
    <actions name="remove_target" scheme="http://occi/tosca/remove_target#" title="Operation to remove a target node."/>
    <actions name="add_target" scheme="http://occi/tosca/add_target#" title="Operation to notify the source node of a target node being added via a relationship."/>
    <actions name="add_source" scheme="http://occi/tosca/add_source#"/>
    <actions name="target_changed" scheme="http://occi/tosca/target_changed#" title="Operation to notify source some property or attribute of the target changed"/>
  </mixins>
  <mixins name="tosca.interfaces.node.lifecycle.Standard" scheme="http://occi/tosca/toscainterfacesnodelifecyclestandard#" depends="//@mixins[term='tosca.interfaces.root']">
    <actions name="create" scheme="http://occi/tosca/create#" title="Standard lifecycle create operation."/>
    <actions name="configure" scheme="http://occi/tosca/configure#" title="Standard lifecycle configure operation."/>
    <actions name="delete" scheme="http://occi/tosca/delete#" title="Standard lifecycle delete operation."/>
  </mixins>
  <mixins name="tosca.relationships.Root" scheme="http://occi/tosca/toscarelationshipsroot#" title="The TOSCA root Relationship Type all other TOSCA base Relationship Types derive from">
    <attributes name="tosca_name" description="This attribute reflects the name of the Relationship Template as defined in the TOSCA service template. This name is not unique to the realized instance model of corresponding deployed application as each template in the model can result in one or more instances (e.g., scaled) when orchestrated to a provider environment." type="//@types.1"/>
    <attributes name="state" default="initial" description="The state of the relationship instance." type="//@types.1"/>
    <attributes name="tosca_id" description="A unique identifier of the realized instance of a Relationship Template that derives from any TOSCA normative type." type="//@types.1"/>
    <applies href="http://schemas.ogf.org/occi/core#//@kinds[term='link']"/>
  </mixins>
  <mixins name="tosca.relationships.ConnectsTo" scheme="http://occi/tosca/toscarelationshipsconnectsto#" title="This type represents a network connection relationship between two nodes." depends="//@mixins[term='tosca.relationships.root']">
    <attributes name="credential" description="The security credential to use to present to the target endpoint to for either authentication or authorization purposes." type="//@types.31"/>
  </mixins>
  <mixins name="tosca.relationships.RoutesTo" scheme="http://occi/tosca/toscarelationshipsroutesto#" title="This type represents an intentional network routing between two Endpoints in different networks." depends="//@mixins[term='tosca.relationships.connectsto']">
    <constraints name="SourceMustBeLoadBalancer"/>
  </mixins>
  <mixins name="tosca.relationships.AttachesTo" scheme="http://occi/tosca/toscarelationshipsattachesto#" depends="//@mixins[term='tosca.relationships.root']">
    <attributes name="location" type="//@types.2"/>
    <constraints name="SourceMustBeComputeAndTargetMustBeBlockStorage"/>
  </mixins>
  <mixins name="tosca.relationships.HostedOn" scheme="http://occi/tosca/toscarelationshipshostedon#" title="This type represents a hosting relationship between two nodes." depends="//@mixins[term='tosca.relationships.root']">
    <constraints name="SourceMustBeSoftwareComponentAndTargetMustBeCompute"/>
    <constraints name="SourceMustBeWebServerAndTargetMustBeWebApplication"/>
    <constraints name="SourceMustBeDatabaseAndTargetMustBeDBMS"/>
    <constraints name="SourceMustBeContainerApplicationAndTargetMustBeContainerRuntime"/>
  </mixins>
  <mixins name="tosca.relationships.DependsOn" scheme="http://occi/tosca/toscarelationshipsdependson#" title="This type represents a general dependency relationship between two nodes." depends="//@mixins[term='tosca.relationships.root']">
    <constraints name="SourceMustBeNodeAndTargetMustBeNode"/>
  </mixins>
  <types xsi:type="occi:RecordType" name="tosca.datatypes.network.PortDef"/>
  <types xsi:type="occi:StringType" name="string"/>
