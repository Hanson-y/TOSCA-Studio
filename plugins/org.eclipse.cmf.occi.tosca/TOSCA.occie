  <types xsi:type="occi:ArrayType" name="arrayDouble" type="//@types.5"/>
  <types xsi:type="occi:ArrayType" name="arrayFloat" type="//@types.6"/>
  <types xsi:type="occi:ArrayType" name="arrayInteger" type="//@types.7"/>
